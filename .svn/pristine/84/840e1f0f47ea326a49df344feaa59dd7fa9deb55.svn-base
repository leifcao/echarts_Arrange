* {
    margin: 0;
    padding: 0
}

html {
    width: 100%;
    height: 100%;
}

body {
    width: 100%;
    height: 100%;
}

body::-webkit-scrollbar {
    width: 6px;
    /* height:px; */
    /**/
}

body::-webkit-scrollbar-track {
    /* background: rgb(201, 201, 201); */
    background: '';
    border-radius: 2px;
}

body::-webkit-scrollbar-thumb {
    background: #bfbfbf;
    border-radius: 10px;
}

body::-webkit-scrollbar-thumb:hover {
    background: #f8f8f8;
    color: #333233;
    /* color: rgb(220, 243, 230) */
    /* color: #333 */
}

body::-webkit-scrollbar-corner {
    background: #434b43;
}

h2 {
    width: 100%;
    height: 42px;
    text-align: left;
    padding-left: 30px;
    box-sizing: border-box;
    color: #cccccc;
    font-size: 16px;
    font-weight: 100;
    letter-spacing: 1px;
    line-height: 42px;
    float: left;
    /* color: #168efe */
}

.pic {
    /* width: 23%;
    height: 232px; */
    width: 28%;
    height: 260px;
    float: left;
    margin-left: 20px;
    background: #ffffff;
    /* background: #262c57; */
    margin-bottom: 20px;
}

.pic .title {
    width: 100%;
    height: 30px;
    box-sizing: border-box;
    padding-left: 12px;
    color: #BDBEC1;
    line-height: 30px;
    border-bottom: 1px solid #2C3468;
}

.picDetial {
    width: 100%;
    height: 100%;
    /* height: 260px; */
}


